<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
		"-//Hibernate/Hibernate Configuration DTD 3.0//EN"
		"http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
    <session-factory>
        <property name="hibernate.connection.driver_class">org.postgresql.Driver</property>
        <property name="hibernate.connection.password">123</property>
        <property name="hibernate.connection.url">jdbc:postgresql://localhost:5432/Contactdb</property>
        <property name="hibernate.connection.username">hp</property>
        <property name="hibernate.default_schema">public</property>
        <property name="hibernate.dialect">org.hibernate.dialect.PostgreSQLDialect</property>
        <property name="current_session_context_class">thread</property>
        <property name="connection.pool_size">23</property>
        <mapping resource="ma/fstm/ilisi/gestioncantact/model/bo/Type.hbm.xml"/>
        <mapping resource="ma/fstm/ilisi/gestioncantact/model/bo/Contact.hbm.xml"/>
    </session-factory>
</hibernate-configuration>
